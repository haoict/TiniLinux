+        vcc5-supply = <&vccsys>;
+        vcc6-supply = <&vccsys>;
+        vcc7-supply = <&vcc_3v0>;
+        vcc8-supply = <&vccsys>;
+        vcc9-supply = <&usb_midu>;
+
+    regulators {
