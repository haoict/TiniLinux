# RGB30 board-specific settings
BR2_TARGET_GENERIC_HOSTNAME="rgb30"
BR2_LINUX_KERNEL_PATCH="$(BR2_EXTERNAL_TiniLinux_PATH)/board/rgb30/linux/patches"
BR2_LINUX_KERNEL_CUSTOM_CONFIG_FILE="$(BR2_EXTERNAL_TiniLinux_PATH)/board/rgb30/linux/linux.conf"
BR2_LINUX_KERNEL_INTREE_DTS_NAME="rockchip/rk3566-powkiddy-rgb30"
BR2_PACKAGE_LINUX_FIRMWARE=y
BR2_PACKAGE_LINUX_FIRMWARE_RTL_88XX_BT=y
BR2_PACKAGE_LINUX_FIRMWARE_RTL_88XX=y
BR2_PACKAGE_LINUX_FIRMWARE_RTL_RTW88=y
BR2_PACKAGE_LINUX_FIRMWARE_RTL_815X=y
BR2_PACKAGE_LINUX_FIRMWARE_RTL_8169=y
BR2_PACKAGE_ROCKCHIP_RKBIN=y
BR2_PACKAGE_ROCKCHIP_RKBIN_TPL_FILENAME="./bin/rk35/rk3568_ddr_1056MHz_v1.23.bin"
BR2_PACKAGE_ROCKCHIP_RKBIN_BL31_FILENAME="./bin/rk35/rk3568_bl31_v1.44.elf"
BR2_TARGET_UBOOT=y
BR2_TARGET_UBOOT_BOARD_DEFCONFIG="anbernic-rgxx3-rk3566"
BR2_TARGET_UBOOT_NEEDS_DTC=y
BR2_TARGET_UBOOT_NEEDS_OPENSSL=y
BR2_TARGET_UBOOT_NEEDS_PYELFTOOLS=y
BR2_TARGET_UBOOT_NEEDS_PYLIBFDT=y
BR2_TARGET_UBOOT_NEEDS_ROCKCHIP_RKBIN=y
# BR2_TARGET_UBOOT_FORMAT_BIN is not set
BR2_PACKAGE_RK3566_DTBO=y
BR2_PACKAGE_HOST_DTC=y
BR2_PACKAGE_HOST_SWIG=y
